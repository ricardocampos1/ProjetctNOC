    <div class="copy" style="background-color: #040319">&copy; 2018 Pyxia.com</div>
    <%--Fim footer--%>
    <script>
        function myFunction() {
            var x = document.getElementById("myTopnav");
            if (x.className === "topnav") {
                x.className += " responsive";
            } else {
                x.className = "topnav";
            }
        }
    </script>
